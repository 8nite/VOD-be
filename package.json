{
  "name": "accedo-be",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all build server",
    "server": "node ./dist-server/app",
    "transpile": "babel ./src --out-dir dist-server",
    "clean": "rimraf dist-server",
    "build": "npm-run-all clean transpile",
    "test": "mocha ./**/*spec.js --reporter spec",
    "cover": "./node_modules/.bin/nyc ./node_modules/.bin/_mocha --require @babel/register \"src/**/*.js\" && get-coverage"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/register": "^7.8.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "cookie-parser": "~1.4.4",
    "debug": "^4.1.1",
    "express": "~4.16.1",
    "mocha": "^7.0.1",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.5",
    "rimraf": "^3.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
